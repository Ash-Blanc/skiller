# LLM Provider API Keys
MISTRAL_API_KEY=your_mistral_api_key_here

# LangWatch
LANGWATCH_API_KEY=your_langwatch_api_key_here

# X/Twitter Scraping - TwitterAPI.io (PRIMARY - Recommended)
# Sign up at https://twitterapi.io and get your API key from the dashboard
# Pricing: ~$0.15/1k requests, very reliable
TWITTER_API_IO_KEY=your_twitterapiio_key_here

# X/Twitter Scraping - Apify (Fallback)
# Sign up at https://apify.com and get your API token from Settings > Integrations
APIFY_API_TOKEN=your_apify_api_token_here

# X/Twitter Scraping - ScrapeGraph (Fallback - AI-powered extraction)
# Sign up at https://scrapegraphai.com for API key
SGAI_API_KEY=your_scrapegraph_api_key_here

# BrightData (Legacy fallback)
BRIGHT_DATA_API_KEY=your_brightdata_api_key_here

# Firecrawl (General web scraping - does NOT work for X.com)
FIRECRAWL_API_KEY=your_firecrawl_api_key_here

# Supermemory (Vector Memory) - Optional
SUPERMEMORY_API_KEY=your_supermemory_api_key_here

# X (Twitter) Official API - DISABLED due to rate limits and paywalls
# Uncomment below if you want to re-enable the X API fallback
# X_BEARER_TOKEN=your_x_bearer_token
# X_CONSUMER_KEY=your_x_consumer_key
# X_CONSUMER_SECRET=your_x_consumer_secret
# X_ACCESS_TOKEN=your_x_access_token
# X_ACCESS_TOKEN_SECRET=your_x_access_token_secret

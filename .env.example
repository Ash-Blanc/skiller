# LLM Provider API Keys
MISTRAL_API_KEY=your_mistral_api_key_here

# LangWatch
LANGWATCH_API_KEY=your_langwatch_api_key_here

# Browserbase (Primary Scraper) - Serverless browser automation
BROWSERBASE_API_KEY=your_browserbase_api_key_here
BROWSERBASE_PROJECT_ID=your_browserbase_project_id_here

# Firecrawl (Fallback Scraper) - Required if no Browserbase keys
FIRECRAWL_API_KEY=your_firecrawl_api_key_here

# Supermemory (Vector Memory) - Optional
SUPERMEMORY_API_KEY=your_supermemory_api_key_here

# X (Twitter) API - DISABLED due to rate limits and paywalls
# Uncomment below if you want to re-enable the X API fallback
# X_BEARER_TOKEN=your_x_bearer_token
# X_CONSUMER_KEY=your_x_consumer_key
# X_CONSUMER_SECRET=your_x_consumer_secret
# X_ACCESS_TOKEN=your_x_access_token
# X_ACCESS_TOKEN_SECRET=your_x_access_token_secret
